  "temporal-pooling": "mean",
  "num-heads-attention-pooling": 1,
  "num-frames": null,
  "has-discriminative-conditioning": false, 
  "space-between-frames": null,
  "epochs": 30,
  "start-train-step": 0,
  "epochs-to-skip": 0,
  "save-every-n-steps": 1e10,
  "continue-from": null,
  "grad-norm-max": 1.0,
  "loss-function": "cross-entropy",
  "dataset-config": {},
  "use-checkpointing": true,
  "debug": false,
  "use-profiling": false,
  "use-fresh-lr-scheduler": false,
  "learn-just-pooling": false,
  "checkpoint-last-n": 12,
  "log-every-n-steps": 10,
  "early-stop-patience": 3,
  "training-history": {
    "train-acc": [],
    "train-loss": [],
    "val-acc": [],
    "val-loss": []
  },
  "fp16": false,
  "grad-scaler-config":{
    "init-scale": 8192,
    "growth-factor": 2.0,
    "backoff-factor": 0.5,
    "growth-interval": 100
  },
  "activation-clamp-value": 1e4,
  "videomix-type": null,
  "videomix-prob": 1,
  "use-augmentation": false
}